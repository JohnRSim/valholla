{
	"background_color": "#000000",
	"theme_color": "#140F26",
	"name": "Val-hölla",
	"short_name": "Val-hölla",
	"display": "standalone",
	"start_url": "./",
	"shortcuts": [
		{
			"name": "Home",
			"short_name": "Home",
			"description": "Launch home dashboard feed",
			"url": "/feed/Home",
			"icons": [{ "src": "/icons/home.png", "sizes": "192x192" }]
		},
		{
			"name": "Apps",
			"short_name": "Apps",
			"description": "Launch Apps",
			"url": "/feed/Apps",
			"icons": [{ "src": "/icons/gmp.png", "sizes": "192x192" }]
		},
		{
			"name": "User Settings",
			"short_name": "Settings",
			"description": "Update your app settings",
			"url": "/settings",
			"icons": [{ "src": "/icons/settings.png", "sizes": "192x192" }]
		}
	],
	"icons": [{
			"src": "maskable_icon-512.png",
			"sizes": "512x512",
			"type": "image/png"
		},
		{
			"purpose": "maskable",
			"src": "maskable_icon-512.png",
			"sizes": "512x512",
			"type": "image/png"
		},
		{
			"src": "maskable_icon-192.png",
			"sizes": "192x192",
			"type": "image/png"
		}
	],
	"related_applications": [{
		"platform": "webapp",
		"url": "https://val-holla.com/manifest.json",
		"id": "valholla.web.pwa"
	}],
	"share_target": {
		"action": "/_share-target",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": {
			"title": "name",
			"text": "description",
			"url": "link",
			"files": [{
				"name": "media",
				"accept": [
				  "audio/*",
				  "image/*",
				  "video/*"
				]
			}]
		}
	}
}
